<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>untangledSystem</display-name>
  <welcome-file-list>
    <welcome-file>/login</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.web.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddRecommendServlet</servlet-name>
    <servlet-class>com.web.AddRecommendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddRecommendServlet</servlet-name>
    <url-pattern>/addRecommend</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddStoryServlet</servlet-name>
    <servlet-class>com.web.AddStoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddStoryServlet</servlet-name>
    <url-pattern>/addStory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FindStoryServlet</servlet-name>
    <servlet-class>com.web.FindStoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindStoryServlet</servlet-name>
    <url-pattern>/findStory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FindPlayServlet</servlet-name>
    <servlet-class>com.web.FindPlayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindPlayServlet</servlet-name>
    <url-pattern>/findPlay</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FindRecommendServlet</servlet-name>
    <servlet-class>com.web.FindRecommendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindRecommendServlet</servlet-name>
    <url-pattern>/findRecommend</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteStoryServlet</servlet-name>
    <servlet-class>com.web.DeleteStoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteStoryServlet</servlet-name>
    <url-pattern>/deleteStory</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteRecommendServlet</servlet-name>
    <servlet-class>com.web.DeleteRecommendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteRecommendServlet</servlet-name>
    <url-pattern>/deleteRecommend</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>com.web.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home.php</url-pattern>
  </servlet-mapping>
</web-app>